{
    "compilerOptions": {
        "strict": true,
        "moduleResolution": "node",
        "noEmit": false,
        "removeComments": true,
        "target": "es5",
        "allowJs": true,
        "noImplicitThis": true,
        "noUnusedParameters": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitOverride": true,
        "experimentalDecorators": true,
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true
    }
}
